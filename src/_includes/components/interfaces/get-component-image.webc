<script webc:type="render" webc:is="template">
  module.exports = function () {
    const getComponent = () => {
      if (this.component === 'Dropdown Menu') {
        return `<image-get-svg src="interfaces/dropdown-menu.svg"></image-get-svg>`
      }
      if (this.component === 'Segmented Controller') {
        return `<image-get-svg src="interfaces/segmented-controller.svg"></image-get-svg>`
      }
    }
    return getComponent();
  }
</script>